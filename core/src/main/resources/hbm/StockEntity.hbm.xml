<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.springapp.model.StockEntity" table="stock" schema="" catalog="learning">
        <id name="stockId">
            <column name="STOCK_ID" sql-type="int unsigned" length="10" not-null="true"/>
        </id>
        <property name="stockCode">
            <column name="STOCK_CODE" sql-type="varchar" length="10" not-null="true"/>
        </property>
        <property name="stockName">
            <column name="STOCK_NAME" sql-type="varchar" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>